<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NotatnikSilowy.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:localization="clr-namespace:NotatnikSilowy.Localization"
    xmlns:view="clr-namespace:NotatnikSilowy.View"
    Title="{localization:Translate AppTitle}">

    <ShellContent
        Title="{localization:Translate HomeTitle}"
        ContentTemplate="{DataTemplate view:MainPage}"
        Route="MainPage" />

</Shell>
